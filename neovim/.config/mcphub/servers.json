{
  "mcpServers": {
    "Notion": {
      "autoApprove": [
        "fetch",
        "notion-get-comments",
        "notion-get-self",
        "notion-get-teams",
        "notion-get-user",
        "notion-get-users",
        "search"
      ],
      "disabled": false,
      "url": "https://mcp.notion.com/mcp"
    },
    "atlassian": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.atlassian.com/v1/sse"
      ],
      "autoApprove": [
        "atlassianUserInfo",
        "fetch",
        "getAccessibleAtlassianResources",
        "getConfluencePage",
        "getConfluencePageDescendants",
        "getConfluencePageFooterComments",
        "getConfluencePageInlineComments",
        "getConfluenceSpaces",
        "getJiraIssue",
        "getJiraIssueRemoteIssueLinks",
        "getJiraIssueTypeMetaWithFields",
        "getJiraProjectIssueTypesMetadata",
        "getPagesInConfluenceSpace",
        "getTransitionsForJiraIssue",
        "getVisibleJiraProjects",
        "lookupJiraAccountId",
        "search",
        "searchConfluenceUsingCql",
        "searchJiraIssuesUsingJql"
      ],
      "command": "npx",
      "disabled": true
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "64000"
      }
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "autoApprove": [
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities",
        "delete_observations",
        "delete_relations",
        "read_graph",
        "search_nodes",
        "open_nodes"
      ],
      "command": "npx"
    },
    "octocode": {
      "args": [
        "mcp",
        "--path",
        "."
      ],
      "autoApprove": [
        "semantic_search",
        "view_signatures",
        "memorize",
        "remember",
        "forget",
        "graphrag"
      ],
      "command": "octocode"
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "disabled": true
    }
  }
}
